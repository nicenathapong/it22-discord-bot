generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id         String    @unique
  program    String
  project    String
  discordId  String?   @unique
  nickname   String?
  note       String?
  verifiedAt DateTime?
}
